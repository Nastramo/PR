#include <iostream>
using namespace std;
int main() {
    setlocale(LC_ALL, "Russian");
    int n;
    cin >> n;
    if ((n >= 1) and (n <= 4))
        cout <<"few";
    else if ((n >= 5) and (n <= 9))
        cout << "several";
    else if ((n >=10) and (n <= 19))
        cout << "pack";
    else if ((n >= 20) and (n <= 49))
        cout << "lots";
    else if ((n >= 50) and (n <= 99))
        cout << "horde";
    else if ((n >= 100) and (n <= 249))
        cout << "throng";
    else if ((n >= 250) and (n <= 499))
        cout << "swarm";
    else if ((n >= 500) and (n <= 999))
        cout << "zounds";
    else if (n >= 1000)
        cout << "legion";
}
